@model IEnumerable<City>
<h1>地域設定</h1>

    <label>都道府県</label>
    <select id="todoufuken" name="todoufuken" onchange="selectArea()">
        <option value="00">都道府県を選択してください</option>
        @foreach(var city in Model)
        {
            <option value="@city.Id">@city.CityName</option>
        })
    </select>
    <label>市区町村</label>
    <select id="cities" name="cities"></select>
    <button type="submit" onclick="getLocate()">設定</button>
