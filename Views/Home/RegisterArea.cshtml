@model IEnumerable<City>
<h1>地域設定</h1>
<form method="post">
    <label>都道府県</label>
    <select>
        @foreach(var city in Model)
        {
            <option value="@city.Id">@city.CityName</option>
        })
    </select>
    <label>市区町村</label>
    <select></select>
    <button type="submit" class="w-100 btn btn-lg btn-primary">設定</button>
</form>